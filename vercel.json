{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.py"
        },
        {
            "src": "/",
            "dest": "/learning.html"
        },
        {
            "src": "/(.*\\.(html|js|json|css|png|jpg|jpeg|gif|svg|ico))",
            "dest": "/$1"
        }
    ]
}